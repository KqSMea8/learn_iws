<ol class="breadcrumb">
  <li><a href="/">home</a></li>
  <li class="active">score</li>
</ol>
<table class="table">
  <tr><th>Type</th><th>Machine</th><th>Item</th><th>Real Value</th><th>Thread Value</th><th>Weight(%)</th><th>Point</th>
    <% check_list.keys.each do |type| %>
      <% len = 0 %>
      <% check_list[type].keys.each do |key| %>
        <% len += check_list[type][key].length + 1 %>
      <% end %>
      <% len = len + 1 %>
      <tr>
        <td rowspan=<%= len %>>
          <%= "#{type}" %>
        </td>
        <% check_list[type].keys.each do |key| %>
          <% length = check_list[type][key].length + 1 %>
          <tr>
            <td rowspan=<%= length %>>
              <%= "#{key}" %>
            </td>
            <% check_list[type][key].keys.each do |item| %>
              <tr><td>
                  <%= "#{item}" %>
                  </td><td>
                  <%= "#{check_list[type][key][item]}".split(':').join('</td><td>') %>
              </td></tr>
            <% end %>
          </tr>
        <% end %>
      </tr>
    <% end %>
  </table>
